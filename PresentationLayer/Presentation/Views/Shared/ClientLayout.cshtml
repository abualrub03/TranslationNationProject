<script type="text/javascript">
    function switchLanguage(langSelect) {
        var selectedLang = langSelect.value;
        var currentUrl = window.location.href.split('?')[0];  // Get the base URL without parameters

        // Redirect to the URL with the selected language parameter
        window.location.href = currentUrl + '?lang=' + selectedLang;
    }
</script>

